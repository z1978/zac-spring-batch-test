<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- 数据库驱动包位置 -->
	
	<classPathEntry	location="C:\Users\ts-ningbo.zhang\.m2\repository\mysql\mysql-connector-java\5.1.30\mysql-connector-java-5.1.30.jar" />
	<!--
	<classPathEntry	location="C:\Users\zhangningbo\.m2\repository\mysql\mysql-connector-java\5.1.30\mysql-connector-java-5.1.30.jar" />
	<classPathEntry	location="/Users/zhangningbo/.m2/repository/mysql/mysql-connector-java/5.1.30/mysql-connector-java-5.1.30.jar" />
	-->
	<context id="context1">
		<commentGenerator>
			<!-- 是否去除自动生成的注释 true：是 ： false:否 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- 数据库链接URL、用户名、密码 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/spring_batch_test" userId="root"
			password="hogehoge" />
		<!-- 生成模型的包名和位置 -->
		<javaModelGenerator targetPackage="com.zac.spring_batch.entity"
			targetProject="zac-spring-batch-test/src/main/java" />
		<!-- 生成的映射文件报名和位置 -->
		<!-- 只有选XMLMAPPER模式的时候才会生成[com.zac.spring_batch.mapper] -->
		<!-- 选ANNOTATEDMAPPER模式的时候Mapper和Sql都会生成到[com.zac.spring_batch.dao] -->
		<sqlMapGenerator targetPackage="com.zac.spring_batch.mapper"
			targetProject="zac-spring-batch-test/src/main/java" />
		<!-- 生成DAO的包名和位置和类型(ANNOTATEDMAPPER/XMLMAPPER) -->
		<javaClientGenerator targetPackage="com.zac.spring_batch.dao" targetProject="zac-spring-batch-test/src/main/java" type="ANNOTATEDMAPPER" />
		<table schema="spring_batch_test" tableName="people"
			domainObjectName="people" enableCountByExample="false"
			enableUpdateByExample="false" enableDeleteByExample="false"
			enableSelectByExample="false" selectByExampleQueryId="false">
			<columnOverride column="first_name" property="first_name" />
			<columnOverride column="last_name" property="last_name" />
		</table>
	</context>
</generatorConfiguration>  